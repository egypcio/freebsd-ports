#!/bin/sh
# $FreeBSD: head/net/corosync3/files/corosync-notifyd.in 411764 2016-03-24 10:14:11Z tijl $

# PROVIDE: corosync-notifyd
# REQUIRE: LOGIN FILESYSTEMS
# KEYWORD: shutdown

. /etc/rc.subr

name="corosync_notifyd"
rcvar="corosync_notifyd_enable"

load_rc_config $name

: ${corosync_notifyd_enable:=NO}

command="%%PREFIX%%/sbin/corosync-notifyd"
command_args="-l"

run_rc_command "$1"
